{
  "name": "infosysbub-studisu",
  "version": "1.64.0",
  "description": "",
  "authors": [
    "Informations-Systeme Bildung und Berufe - INFOSYSBUB"
  ],
  "license": "Copyright 2018 - Bundesagentur fuer Arbeit - all rights reserved",
  "repository": {
    "type": "git",
    "url": "http://stash.webapp.sdst.sbaintern.de/scm/infosysbub/infosysbub-studisu.git"
  },
  "keywords": [
    "angular",
    "Studienangebote",
    "Studiensuche",
    "InfoSysBuB"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --deploy-url /studiensuche/",
    "start:aot": "ng serve --aot --deploy-url /studiensuche/",
    "start:prod": "ng serve --aot --prod --deploy-url /studiensuche/",
    "build": "ng lint && npm run gen_image_map && ng build",
    "build:prod": "ng lint && npm run clean:dist && npm run gen_image_map && ng build --prod --aot",
    "build:aot": "npm run build:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "rimraf": "rimraf",
    "typedoc": "typedoc",
    "postinstall": "rimraf node_modules/@angular-devkit/build-angular/node_modules/uglify-es && rimraf node_modules/@angular-devkit/build-angular/node_modules/uglifyjs-webpack-plugin",
    "gen_image_map": "node generate-image-map.js"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "bapf-http": "^5.0.0",
    "bapf-oag": "1.3.1",
    "bapf-pattern-library": "2.1.2",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "rxjs-compat": "6.2.0",
    "smoothscroll-polyfill": "0.4.0",
    "zone.js": "^0.8.26",
    "postinstall": "*"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-optimizer": "0.0.32",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@compodoc/compodoc": "1.1.3",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "ajv": "6.5.1",
    "codelyzer": "~4.2.1",
    "fs": "0.0.1-security",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "rimraf": "2.6.2",
    "uglify-es": "3.2.2",
    "uglifyjs-webpack-plugin": "1.1.5",
    "tslint": "~5.9.1",
    "typedoc": "0.7.2",
    "typescript": "~2.7.2"
  },
  "publishConfig": {
    "registry": "http://nexus.webapp.sdst.sbaintern.de:9091/repository/npm_hosted"
  },
  "engines": {
    "node": ">= 8.11.1",
    "npm": ">= 5.6.0"
  }
}
